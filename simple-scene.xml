<?xml version="1.0" encoding="utf-8"?>
<rt>
  <image>
    <width>800</width>
    <height>600</height>
  </image>
  <camera>
    <position>0 0 10</position>
    <to>0 0 0</to>
    <up>0 1 0</up>
    <angle>60</angle>
    <aspect>16:9</aspect>
    <projection>perspective</projection>
  </camera>
  <scene name="simple-scene">
    <background>0 0 0</background>
    <ambient>0.5 0.5 0.5</ambient>
    <mesh file="missil.obj">
      <material>
        <ambient>0.2 0.2 0.2</ambient>
        <diffuse>0.5 0 0</diffuse>
        <spot>0.3 0 0</spot>
        <shine>10</shine>
      </material>
      <transform>
         <position>0 0 0</position>
         <scale>0.2 0.2 0.2</scale>
      </transform>
    </mesh>
    <sphere>
      <center>0 1 0</center>
      <radius>1</radius>
      <height>2 3 2</height>
    </sphere>
    <cylinder>
      <center>1 1 2</center>
      <radius>1</radius>
    </cylinder>
     <box>
      <center>5 0 0</center>
    </box>
    <box>
      <scale>0.2 0.2 0.2</scale>
      <center>2 1 0</center>
    </box>
    <cylinder>
    <light type="directional">
      <direction>-1 0 0</direction>
    </light>
  </scene>
</rt>